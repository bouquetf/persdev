<p id="notice"><%= notice %></p>

<h1>Listing Day Agendas</h1>

<table>
  <thead>
    <tr>
      <th>Date</th>
      <th>Agenda</th>
      <th>Quote</th>
      <th>Targets</th>
      <th>Actions</th>
      <th>Success</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @day_agendas.each do |day_agenda| %>
      <tr>
        <td><%= day_agenda.date %></td>
        <td><%= day_agenda.agenda %></td>
        <td><%= day_agenda.quote %></td>
        <td><%= day_agenda.targets %></td>
        <td><%= day_agenda.actions %></td>
        <td><%= day_agenda.success %></td>
        <td><%= day_agenda.user %></td>
        <td><%= link_to 'Show', day_agenda %></td>
        <td><%= link_to 'Edit', edit_day_agenda_path(day_agenda) %></td>
        <td><%= link_to 'Destroy', day_agenda, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Day agenda', new_day_agenda_path %>
